programa {
	funcao inicio() {
	    
	    //Declaração de Variáveis
	    
	    cadeia frutas[4] //Declara um vetor de 4 posições
	    inteiro contador = 0
	    
	    frutas[0] = "Maça"
	    frutas[1] = "Pera"
	    frutas[2] = "Uva"
	    frutas[3] = "Jaca"
	    
	    faca {
	        
	        escreva(frutas[contador] + "\n")
	        contador++
	        
	    } enquanto (contador <= 3)
		
	}
}
